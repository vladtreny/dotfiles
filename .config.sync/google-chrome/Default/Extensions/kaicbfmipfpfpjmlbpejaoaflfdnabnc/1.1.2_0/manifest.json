{
   "background": {
      "scripts": [ "js/bootstrap.js" ]
   },
   "content_scripts": [ {
      "js": [ "js/redirect.js" ],
      "matches": [ "*://www.bing.com/search*" ]
   } ],
   "description": "Redirect all Bing (and therefore Cortana) searches to a search engine of your choice!",
   "icons": {
      "128": "images/logo-128.png",
      "32": "images/logo-32.png",
      "64": "images/logo-64.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Chrometana - Redirect Bing Somewhere Better",
   "options_page": "html/options.html",
   "page_action": {
      "default_title": "Chrometana"
   },
   "permissions": [ "*://www.bing.com/", "webRequest", "webRequestBlocking", "storage", "background" ],
   "short_name": "Chrometana",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.1.2"
}
